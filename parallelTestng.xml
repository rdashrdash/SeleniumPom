<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Naukri website Test" parallel="methods">

	<listeners>
		<listener class-name="com.qa.naukri.extentReportListener.ExtentReportListener" />
		<listener class-name="com.naukri.qa.analyzer.Transform" />
	</listeners>

	
	<test name="Naukri website test cases">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.naukri.qa.testcases.LandingPageTest"></class>
			<class name="com.naukri.qa.testcases.SearchjobsPageTest"></class>
		</classes>
	</test>
	
	
</suite>
